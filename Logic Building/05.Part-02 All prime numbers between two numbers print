/*

#include <iostream>
using namespace std;
int main() {
   
   int i,j,mid,num,sum1=0,sum2=0;

    cout<<"Start = ";
    cin>>i;
    cout<<"end = ";
    cin>>num;
   
    for( i=i ;i<=num ;i++)
   {
         
        if(i>1)
        {
        mid=i/2;
        
        for(j = 2; j <= mid; j++)
            {
                
            //Nonprime
             if(i%j==0)
                {
                    sum2-=i;
                    break;
                }
            }
            //Prime sum
            sum2 +=i;
            
            if(sum1!=sum2)
            cout<<i<<" ";
            
            sum1=sum2;
            
       }
   }
   
    return 0;
}

*/


